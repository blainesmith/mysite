<div id="about">
    <div class="wrap">
        <div class="container-fluid">
            @Html.Partial("NavAccessoryLayout")
           
        <div class="row text-center animated fadeInDownBig">
            <div class="col-md-12">
                <h1>Get in touch</h1>
                <p>Lorem ipsum dolor sit amet, consectetur adipisicing elit, sed do eiusmod tempor</p>
            </div>
        </div>
        <div class="row animated fadeInLeftBig">
            <div class="col-md-2 col-md-offset-1">
                <address>
                  <strong>Blaine Smith</strong><br>
                  <a href="mailto:#">blaine@blainesmith.net</a>
                </address>
